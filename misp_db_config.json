{
    "database": {
        "host": "localhost",
        "port": 3306,
        "user": "YOUR_USERNAME",
        "password": "YOUR_PASSWORD",
        "database": "misp"
    },
    "extraction": {
        "hours_lookback": 24,
        "ioc_types": [
            "ip-src", "ip-dst", "domain", "hostname", "url", 
            "md5", "sha1", "sha256", "filename", "email-src", 
            "email-dst", "mutex", "regkey", "snort", "yara"
        ]
    },
    "output": {
        "json_file": "misp_recent_iocs.json",
        "cache_db": "ioc_cache.db"
    }
}
